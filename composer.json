{
    "name": "drabbit/color-semantics",
    "description": "",
    "keywords": [
        "drabbit",
        "color-semantics"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "drunkrabbit1",
            "email": "rashid5742zg@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/collections": "^9.34"
    },
    "autoload": {
        "psr-4": {
            "Drabbit\\ColorSemantics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drabbit\\ColorSemantics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drabbit\\ColorSemantics\\ColorSemanticsServiceProvider"
            ],
            "aliases": {
                "ColorSemantics": "Drabbit\\ColorSemantics\\ColorSemanticsFacade"
            }
        }
    }
}
